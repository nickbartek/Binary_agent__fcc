function binaryAgent(str) {
let arr=str.split(' ')
let arr2=[]
let res=0;
function div(x){
    let inside=arr[x].split('')
    let m=128
    res=0;
    for (let a=0; a<inside.length; a++){
      if(inside[a]==='1'){res+=m}
      m= m*0.5 ;  
      }return res}



for(let b=0; b<arr.length; b++){
arr2.push(String.fromCharCode(div(b)))


}
str=arr2.join('');
  return str;}
